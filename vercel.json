{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "out"
      }
    }
  ],
  "routes": [
    { "src": "/login/?$", "dest": "/login.html" },
    { "src": "/(.*)", "dest": "/$1.html" }
  ],
  "outputDirectory": "out",
  "buildCommand": "npm run build"
}
